{
    "version": 2,
    "builds": [
      {
        "src": "dist/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/chat",
        "methods": ["POST"],
        "dest": "dist/server.js"
      },
      {
        "src": "/health",
        "methods": ["GET"],
        "dest": "dist/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "public/$1"
      }
    ]
  }